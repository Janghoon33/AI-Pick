{
  "version": 2,
  "installCommand": "npm install",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["config/**", "controllers/**", "middleware/**", "models/**", "routes/**", "service/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
